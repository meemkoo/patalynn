{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Patalynn Main",
            "type": "debugpy",
            "request": "launch",
            "module": "patalynn",
            "env": {
                "PYTHONPATH": "${workspaceRoot}/src"
            },
            "args": [".\\test1\\test2\\"]
        }
    ]
}